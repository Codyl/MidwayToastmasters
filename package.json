{
  "name": "toastmasters",
  "version": "1.0.0",
  "description": "",
  "main": "client/assets/src/index.js",
  "scripts": {
    "lite": "lite-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "sass --watch client/assets/scss/main.scss client/assets/css/sassy.css",
    "watch:scss": "onchange \"client/assets/scss/**/*.scss\" --npm run sass",
    "watch:all": "parallelshell \" npm run sass\" \"npm run lite\"",
    "clean": "rimraf dist",
    "usemin": "usemin ./assets/src/index.html -d dist --htmlmin -o dist/index.html",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* -o dist/img",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "bootstrap": "^4.6.0",
    "fontawesome": "^4.7.0",
    "jquery": "^3.5.1",
    "onchange": "^7.0.0",
    "parallelshell": "^3.0.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "lite-server": "^2.6.1",
    "sass": "^1.43.2"
  }
}
