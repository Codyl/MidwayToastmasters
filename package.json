{
  "name": "toastmasters",
  "version": "1.0.0",
  "scripts": {
    "lite": "lite-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "sass --watch assets/scss/main.scss client/assets/css/sassy.css",
    "watch:scss": "onchange \"assets/scss/**/*.scss\" -- npm run sass",
    "watch:all": "parallelshell \" npm run sass\" \"npm run lite\"",
    "clean": "rimraf dist",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts/",
    "imagemin": "imagemin assets/images/**/* -o dist/images",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "bootstrap": "^4.6.0",
    "copyfiles": "^2.4.1",
    "font-awesome": "^4.7.0",
    "fontawesome": "^4.7.0",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "jquery": "^3.5.1",
    "onchange": "^7.0.0",
    "parallelshell": "^3.0.1",
    "popper.js": "^1.16.1",
    "rimraf": "^3.0.2",
    "usemin": "^0.6.0",
    "usemin-cli": "^0.6.0"
  },
  "devDependencies": {
    "lite-server": "^2.6.1",
    "sass": "^1.43.2"
  }
}
